.invitemar{margin-top:10px;border-top:1px solid #f0f0f0;}





/*提示*/
.tipsurplusmask{width:100%;height:100%;position:fixed;top:0;left:0;background:rgba(0,0,0,0.5);-webkit-transform:translate3d(0, 0, 0);pointer-events:auto;z-index:2}
.tipsurplus{position:fixed;top:50%;left:20px;right:20px;background-color:#fff;z-index:3;-webkit-border-radius:10px;border-radius:10px;margin-top:-130px}
.tipsurplus h2{text-align:center;font-weight:normal;font-size:16px;padding:10px 0}
.tipsurplus dl{text-align:center;padding:0 10px 10px}
.tipsurplus dl dt{line-height:1.4;padding:10px 0}
.tiptipsurplusBt{display:-webkit-box;-webkit-box-orient:horizontal;-webkit-box-pack:center;-webkit-box-align:center;border-top:1px solid #f0f0f0;}
.tiptipsurplusBt div{-webkit-box-flex:1;width:50%;font-size:16px;color:#69acfa;text-align:center;height:40px;display:-webkit-box;-webkit-box-orient:horizontal;-webkit-box-pack:center;-webkit-box-align:center;}
.tiptipsurplusBt div:last-of-type{background-color:#398bee;color:#fff}
.tiptipsurplusBt div  a{color:#fff}

























