@charset "utf-8";

/*剩余次数*/
.surplusnum{padding:10px;background-color:#fff}


/*模版列表*/
.template{background-color:#fff}
.template ul li{}
.template ul li a{display:block}
.template ul li  div.templcon{display:-webkit-box;padding:5px 0 0px 10px}
.template ul li  div.templcon div.templconInfo{-webkit-box-flex:1;display:-webkit-box;-webkit-box-orient:horizontal;-webkit-box-pack:center;-webkit-box-align:center;border-bottom:1px solid #f0f0f0;margin-left:10px;padding:10px 10px 10px 0;color:#ff2d15}
.template ul li  div.templcon div.templconInfo div{-webkit-box-flex:1;color:#999;font-size:12px}
.template ul li  div.templcon div h2{font-size:18px;font-weight: normal;margin-bottom:10px;color:#333;font-weight:bold}
.template ul li  span.reward{font-size:20px;display:block}
.template ul li  span.waittip{font-size:16px;display:block;color:#ff6857;}
.template ul li  div.templcon span.tipbor{border:1px solid #efeff0;display:inline-block;font-size:12px;padding:3px 10px;-webkit-border-radius:10px;border-radius:10px;margin-right:5px;background-color:#f8f9fb;color:#808080}
.template ul li:last-of-type a div.templcon div.templconInfo{border-bottom:0 none}
.template ul li  div.templcon div.templconInfo div.signhandle{display:inline-block;color:#ff2d15}







